#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    int arr[100];
    cout << "input size\n";
    cin >> n;

    for(int i=0;i<n;++i) {
        cin >> arr[i];
    }

    reverse(arr[0], arr + n); // reverse is because #include<algorithm>
    cout << "after reverse " << endl;
    for(int i=0;i<n;++i) {
            cout << arr[i] << " ";

    }

    return 0;
}
